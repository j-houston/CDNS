config globals 'globals'
	option ifbname 'ifb1'
	option dhcpleases '/tmp/dhcp.leases'
	option storage '/tmp/testing'
	option max_capture_size '1M'
	option max_capture_time '60'
	option default_profile 'full'
	list interface 'lan'
	list allowed_subnets '192.168.1.0/24'

config profile 'brazil3g'
	option name 'Brazil (3G)'
	option in 'rate 1550kbit'
	option in_netem 'delay 150ms 50ms 25% loss 1% 40%'
	option out 'rate 460kbit'
	option out_netem 'delay 150ms 50ms 25% loss 1% 40%'

config profile 'india2g'
	option name 'India (2G)'
	option in 'rate 550kbit'
	option in_netem 'delay 300ms 150ms 40% loss 3% 40%'
	option out 'rate 75kbit'
	option out_netem 'delay 300ms 150ms 40% loss 3% 40%'
	
config profile 'india3g'
	option name 'India (3G)'
	option in 'rate 4Mbit'
	option in_netem 'delay 200ms 50ms 40% loss 2% 40%'
	option out 'rate 1Mbit'
	option out_netem 'delay 200ms 50ms 40% loss 2% 40%'
	
config profile 'india3gSpotty'
	option name 'India (Spotty 3G)'
	option in 'rate 4Mbit'
	option in_netem 'delay 200ms 500ms 75% loss 15% 80%'
	option out 'rate 1Mbit'
	option out_netem 'delay 200ms 500ms 75% loss 15% 80%'
	
config profile 'thailand3G'
	option name 'Thailand (3G)'
	option in 'rate 7750kbit'
	option in_netem 'delay 60ms 200ms 40% loss 1% 40%'
	option out 'rate 200kbit'
	option out_netem 'delay 60ms 200ms 40% loss 1% 40%'

config profile 'myanmarWiFi'
	option name 'Myanmar WiFi'
	option in 'rate 1550kbit'
	option in_netem 'delay 10ms 50ms 40%'
	option out 'rate 210kbit'
	option out_netem 'delay 10ms 50ms 40%'

config profile 'full'
	option name 'Full'
	option in 'rate 100Mbit'
	option in_netem 'delay 0ms'
	option out 'rate 100Mbit'
	option out_netem 'delay 0ms'

