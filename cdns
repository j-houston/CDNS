	option in 'rate 200kbit'
	option in_netem 'delay 120ms 40ms'
	option out 'rate 260kbit'
	option out_netem 'delay 120ms 40ms'

config profile 'gprs'
	option name 'GPRS'
	option in 'rate 80kbit'
	option in_netem 'delay 200ms 40ms'
	option out 'rate 20kbit'
	option out_netem 'delay 200ms 40ms'

config profile 'disabled'
	option name 'Disabled'
	option in 'rate 1000bit'
	option in_netem 'loss 100%'
	option out 'rate 1000bit'
	option out_netem 'loss 100%'

